{
  "dashboard": {
    "title": "RAG Overview",
    "tags": ["genai", "rag", "observability"],
    "timezone": "browser",
    "editable": true,
    "hideControls": false,
    "graphTooltip": 1,
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "type": "timeseries",
        "title": "Request Rate by Operation",
        "targets": [
          {
            "expr": "sum(rate(gen_ai_requests_total[5m])) by (operation)",
            "legendFormat": "{{operation}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {"lineWidth": 2, "fillOpacity": 10},
            "unit": "reqps"
          }
        }
      },
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "type": "timeseries",
        "title": "P95 Latency by Operation",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(gen_ai_request_duration_bucket[5m])) by (operation, le))",
            "legendFormat": "{{operation}} p95",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {"lineWidth": 2},
            "unit": "ms"
          }
        }
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8},
        "type": "stat",
        "title": "Total Requests (24h)",
        "targets": [
          {
            "expr": "sum(increase(gen_ai_requests_total[24h]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 10000, "color": "yellow"},
                {"value": 50000, "color": "red"}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8},
        "type": "stat",
        "title": "Error Rate (5m)",
        "targets": [
          {
            "expr": "sum(rate(gen_ai_requests_total{status=\"error\"}[5m])) / sum(rate(gen_ai_requests_total[5m])) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 5, "color": "red"}
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8},
        "type": "stat",
        "title": "Avg Hit@K (Retrieval)",
        "targets": [
          {
            "expr": "avg(gen_ai_retrieval_hit_at_k)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 0.7, "color": "yellow"},
                {"value": 0.9, "color": "green"}
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        }
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "type": "timeseries",
        "title": "Token Usage by Model",
        "targets": [
          {
            "expr": "sum(rate(gen_ai_tokens_total[5m])) by (model, token_type)",
            "legendFormat": "{{model}} - {{token_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {"lineWidth": 2, "fillOpacity": 0, "stacking": {"mode": "normal"}},
            "unit": "tps"
          }
        }
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "type": "timeseries",
        "title": "Retrieval Results Distribution",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(gen_ai_retrieval_results_count_bucket[5m])) by (le))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(gen_ai_retrieval_results_count_bucket[5m])) by (le))",
            "legendFormat": "p95",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {"lineWidth": 2},
            "unit": "short"
          }
        }
      },
      {
        "id": 8,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24},
        "type": "table",
        "title": "Top Errors (Last Hour)",
        "targets": [
          {
            "expr": "topk(10, sum(increase(gen_ai_requests_total{status=\"error\"}[1h])) by (operation, error_type))",
            "format": "table",
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true},
              "indexByName": {},
              "renameByName": {
                "operation": "Operation",
                "error_type": "Error Type",
                "Value": "Count"
              }
            }
          }
        ]
      }
    ],
    "refresh": "10s",
    "schemaVersion": 38,
    "style": "dark",
    "time": {"from": "now-1h", "to": "now"},
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
    },
    "version": 1
  }
}
